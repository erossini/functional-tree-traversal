<div id="body">
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>Binary Tree</h1>
                <h2>Example to use binary tree with C#, ASP.NET e SQL Server</h2>
            </hgroup>
            <p>
                ASP.NET Web API allows you to expose your applications, data and services to the
                web directly over HTTP.
            </p>
        </div>
    </section>
    <section class="content-wrapper main-content clear-fix">
        <h2>Search by age</h2>
        <input type="text" id="age" size="5" />
        <input type="button" value="Search" onclick="find();" />
        <p id="result" />
    </section>
</div>

 <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.3.min.js"></script>
 <script>
      var uri = 'api/MyApi';

      function find() {
          var id = $('#age').val();
          $.getJSON(uri + '?age=' + id + "&name=")
              .done(function (data) {
                  $('#result').text(data);
              })
              .fail(function (jqXHR, textStatus, err) {
                  $('#result').text('Error: ' + err);
              });
      }
  </script>
